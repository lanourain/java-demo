<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="beanDemo" class="beanfactory.IoC.BeanDemo" init-method="init" >
        <property name="name" value="test1"/>
    </bean>

    <bean id="beanDemoAware" class="beanfactory.IoC.BeanDemoAware" init-method="init" >
        <property name="name" value="testAware"/>
    </bean>
    <bean id="beanBeanPostProcessor" class="beanfactory.IoC.BeanBeanPostProcessor"/>
    <bean id="beanFactoryPostProcessorDemo" class="beanfactory.IoC.BeanFactoryPostProcessorDemo"/>
</beans>